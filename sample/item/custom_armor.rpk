def filename = "textures/custom_armor"
def name = "custom armor"

match {
    "$(DISPLAY).Name" = "ipattern:*$(name)*"
} replace {
    items {
        "*_helmet"     = "$(filename)_helmet"
        "*_chestplate" = "$(filename)_chestplate"
    }
    items {
        "*_leggings"
        "*_boots"
    } = "$(filename)"
}